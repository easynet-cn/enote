{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:write-all",
    "fs:allow-write-text-file",
    "shell:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open"
  ]
}